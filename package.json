{
  "name": "canvas-picture-in-picture",
  "version": "0.1.0",
  "description": "A plugin to allow putting Canvas nodes in picture-in-picture mode",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "typecheck": "tsc -noEmit -skipLibCheck",
