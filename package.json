    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "electron": "^25.0.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "prettier": "3.2.5",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vitepress": "^1.1.4",
    "vue": "^3.4.26"
  },
